{"ast":null,"code":"export default {\n  name: \"paper-table\",\n  props: {\n    columns: Array,\n    data: Array,\n    type: {\n      type: String,\n      // striped | hover\n      default: \"striped\"\n    },\n    title: {\n      type: String,\n      default: \"\"\n    },\n    subTitle: {\n      type: String,\n      default: \"\"\n    }\n  },\n  computed: {\n    tableClass() {\n      return `table-${this.type}`;\n    }\n  },\n  methods: {\n    hasValue(item, column) {\n      return item[column.toLowerCase()] !== \"undefined\";\n    },\n    itemValue(item, column) {\n      return item[column.toLowerCase()];\n    }\n  }\n};","map":{"version":3,"names":["name","props","columns","Array","data","type","String","default","title","subTitle","computed","tableClass","methods","hasValue","item","column","toLowerCase","itemValue"],"sources":["src/components/PaperTable.vue"],"sourcesContent":["<template>\n  <table class=\"table\" :class=\"tableClass\">\n    <thead>\n      <slot name=\"columns\">\n        <th v-for=\"column in columns\" :key=\"column\">{{ column }}</th>\n        <th>Status</th>\n        <th>Edit</th>\n      </slot>\n    </thead>\n    <tbody>\n      <tr v-for=\"(item, index) in data\" :key=\"index\">\n        <slot :row=\"item\">\n          <td\n            v-for=\"(column, index) in columns\"\n            :key=\"index\"\n            v-if=\"hasValue(item, column)\"\n          >\n            {{ itemValue(item, column) }}\n          </td>\n          <td>\n            <span v-if=\"item.status === 'active'\" class=\"badge bg-success\">Active</span>\n            <span v-else class=\"badge bg-danger\">Inactive</span>\n          </td>\n            <td>\n            <!-- Nút Edit -->\n            <button @click=\"editItem(item)\" class=\"ti-cut btn btn-warning btnLeft\"></button>\n            <!-- Nút Delete -->\n            <button @click=\"deleteItem(item)\" class=\"ti-close btn btn-danger\"></button>\n            </td>\n        </slot>\n      </tr>\n    </tbody>\n  </table>\n</template>\n<script>\nexport default {\n  name: \"paper-table\",\n  props: {\n    columns: Array,\n    data: Array,\n    type: {\n      type: String, // striped | hover\n      default: \"striped\",\n    },\n    title: {\n      type: String,\n      default: \"\",\n    },\n    subTitle: {\n      type: String,\n      default: \"\",\n    },\n  },\n  computed: {\n    tableClass() {\n      return `table-${this.type}`;\n    },\n  },\n  methods: {\n    hasValue(item, column) {\n      return item[column.toLowerCase()] !== \"undefined\";\n    },\n    itemValue(item, column) {\n      return item[column.toLowerCase()];\n    },\n    \n  },\n};\n</script>\n<style>\n.btnLeft {\n  margin-right:10px;\n}\n\n.badge {\n  text-transform: uppercase;\n  font-size: small !important;\n  color: rgb(255, 255, 255);\n\n}\n</style>\n"],"mappings":"AAmCA;EACAA,IAAA;EACAC,KAAA;IACAC,OAAA,EAAAC,KAAA;IACAC,IAAA,EAAAD,KAAA;IACAE,IAAA;MACAA,IAAA,EAAAC,MAAA;MAAA;MACAC,OAAA;IACA;IACAC,KAAA;MACAH,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;IACAE,QAAA;MACAJ,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;EACA;EACAG,QAAA;IACAC,WAAA;MACA,qBAAAN,IAAA;IACA;EACA;EACAO,OAAA;IACAC,SAAAC,IAAA,EAAAC,MAAA;MACA,OAAAD,IAAA,CAAAC,MAAA,CAAAC,WAAA;IACA;IACAC,UAAAH,IAAA,EAAAC,MAAA;MACA,OAAAD,IAAA,CAAAC,MAAA,CAAAC,WAAA;IACA;EAEA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}